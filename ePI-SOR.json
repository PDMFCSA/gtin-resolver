    "/integration/{domain}/listProducts/{start}/{number}/{sort}/{query}": {
      "get": {
        "tags": [
          "DSU Fabric"
        ],
        "summary": "List products",
        "description": "Return a list of products and metadata required to display in the UI",
        "operationId": "listProducts",
        "parameters": [
          {
            "name": "domain",
            "in": "path",
            "description": "The blockchain domain",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "start",
            "in": "path",
            "description": "Index of the product which is going to be listed first",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64",
              "example": 10
            }
          },
          {
            "name": "number",
            "in": "path",
            "description": "The number of products to be listed",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64",
              "example": 10
            }
          },
          {
            "name": "sort",
            "in": "path",
            "description": "The sorting criteria",
            "required": true,
            "schema": {
              "type": "string",
              "example": "asc",
              "enum": [
                "asc",
                "desc"
              ]
            }
          },
          {
            "name": "query",
            "in": "path",
            "description": "The search criteria",
            "required": true,
            "schema": {
              "type": "string",
              "example": "gtin == 00000000000000"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Operation"
          },
          "400": {
            "description": "Invalid Parameter"
          }
        }
      }
    },
